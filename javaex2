class exw
{
    int i=12;
    void call()
    {
        System.out.println("i in call "+i);
    }
}
class javaex2
{
    public static void main(String args[])
    {
        System.out.println("i in exw "+new exw().i);
        new exw().call();
        System.out.println("hello this javaex2.java is changed");
 System.out.println("code changed and pushed again");
        
    }
}
